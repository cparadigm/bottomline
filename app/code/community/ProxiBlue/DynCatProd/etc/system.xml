<?xml version="1.0"?>
<config>
    <tabs>
        <proxiblue translate="label" module="dyncatprod">
            <label><![CDATA[<div style="background:url(data:image/png;base64,) no-repeat; width: 155px; height: 20px;"></div>]]></label>
            <sort_order>10000000</sort_order>
        </proxiblue>
    </tabs>
    <sections>
        <dyncatprod translate="label" module="dyncatprod">
            <label><![CDATA[<script>document.write('<div style="background:url(https://www.proxiblue.com.au/moduleicons/fetch/icon/name/dyncatprod/domain/'+window.location.hostname+'/key/zOLbXFGQfOErWZicd96azw%3D%3D) no-repeat; padding-left: 20px;">Dynamic Category Products</div>');</script>]]></label>
            <tab>proxiblue</tab>
            <frontend_type>text</frontend_type>
            <sort_order>342</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <hint>
                    <frontend_model>dyncatprod/adminhtml_system_config_fieldset_hint</frontend_model>
                    <sort_order>0</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                </hint>
                <rebuild translate="label">
                    <label>Rebuild Options</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <product_save translate="label">
                            <label>Disable Dynamic Rebuild on Product Save</label>
                            <comment><![CDATA[If a product attribute is changed, all categories with that attribute in dynamic rules will be rebuild in a 10 minute cron schedule. If this is disabled, you will need to wait until automated daily rebuild is run at 00:00:00]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </product_save>
                        <max_exec translate="label">
                            <label>Disable Time To Execute Workaround</label>
                            <comment><![CDATA[Some dynamic rules can result in thousands of products to process. Dynamic Products Save routines enables PHP ability to do lengthy operations. Depending on your hosting options, this may not be allowed. <br/>Disable this feature if you get errors regarding setting max_execution time at run time.]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>3</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </max_exec>
                        <max_exec_time translate="label">
                            <label>Time To Execute Workaround</label>
                            <comment><![CDATA[If you have a large catalog, you may need to increase the processing time above the default 1 hour (3600 seconds)]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </max_exec_time>
                        <delayed translate="label">
                            <label>Delay build of category dynamic products on category save</label>
                            <comment><![CDATA[Due to rules and the size of your catalog, some categories may time out when saving the category, as the rules take some time to proces all the products. Enable this option to allow a delayed build, which will build the category products in a 5 minute cycle.]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </delayed>
                        <disable_indexers translate="label">
                            <label>Disable indexing whilst cron jobs run</label>
                            <comment><![CDATA[Indexing can slow the rebuild of dynamic categories down. Use this option to temporarily set indexing to manual whilst the dynamic categories are built.]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>5</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </disable_indexers>
                        <ignore_indexers translate="label">
                            <label>Completely ignore indexing</label>
                            <comment><![CDATA[The module will not deal with indexing at all.]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>5</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </ignore_indexers>
                    </fields>
                </rebuild>
                <ept translate="label">
                    <label>Enhanced Product Tab</label>
                    <comment><![CDATA[This feature is only available in magento CE 1.6+ or EE 1.12+]]></comment>
                    <frontend_type>text</frontend_type>
                    <sort_order>2</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label">
                            <label>Enable</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enabled>
                        <image_size translate="label">
                            <label>Image Size</label>
                            <comment><![CDATA[Change the size of the image display in the admin grid]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </image_size>
                        <!--<load_jquery translate="label">
                            <label>Load jQuery</label>
                            <comment><![CDATA[If you already have jQuery loaded, or use magento CE 1.9+ or EE 1.14+, set this to no]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </load_jquery>-->
                    </fields>
                </ept>
                <notify translate="label">
                    <label>Notification Options</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>2</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label">
                            <label>Send email notification if category has less than XX products</label>
                            <comment><![CDATA[Email a notification when the daily build runs at 00:00:00 and the category has less than the 'notification product count' found]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enabled>
                        <product_notify_count>
                            <label>Notification Product Count</label>
                            <comment><![CDATA[If products found in daily dynamic cron build is less than this number, send notification]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <depends>
                                <enabled>1</enabled>
                            </depends>
                        </product_notify_count>
                        <category_disabled translate="label">
                            <label>Send email notification if category is disabled</label>
                            <comment><![CDATA[Email a notification when the daily build runs at 00:00:00 and a category is disabled via the category control rules]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>3</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </category_disabled>
                        <identity_from translate="label">
                            <label>Sender of Emails</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_email_identity</source_model>
                            <sort_order>400</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </identity_from>
                        <identity_to translate="label">
                            <label>Receiver of Emails</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_email_identity</source_model>
                            <sort_order>401</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </identity_to>
                    </fields>
                </notify>
                <debug translate="label">
                    <label>Debug Options</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10000</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label">
                            <label>Enabled debug messages to system.log</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enabled>
                    </fields>
                </debug>
            </groups>
        </dyncatprod>
    </sections>
</config>
